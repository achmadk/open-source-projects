{
  "version": "1.0.2",
  "license": "MIT",
  "type": "module",
  "main": "dist/index.umd.cjs",
  "typings": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.umd.cjs",
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "sideEffects": false,
  "files": ["dist"],
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "start": "dts watch",
    "build": "vite build",
    "test": "vitest run",
    "lint": "biome check --organize-imports-enabled=false --no-errors-on-unmatched --skip-errors --write --unsafe",
    "prepublishOnly": "pnpm run build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "dependencies": {
    "@scarf/scarf": "1.3.0"
  },
  "peerDependencies": {
    "@abraham/reflection": "0.12.0",
    "@achmadk/inversify": "workspace:../inversify"
  },
  "publishConfig": {
    "access": "public"
  },
  "name": "@achmadk/inversify-props",
  "author": "Achmad Kurnianto",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/achmadk/open-source-projects.git",
    "directory": "packages/inversify-props"
  },
  "module": "./dist/index.js",
  "size-limit": [
    {
      "path": "./dist/index.js",
      "limit": "400 B"
    },
    {
      "path": "./dist/index.umd.cjs",
      "limit": "2 KB"
    }
  ],
  "homepage": "https://github.com/achmadk/open-source-projects/tree/main/packages/inversify-props#readme",
  "devDependencies": {
    "@achmadk/inversify": "workspace:../inversify"
  },
  "keywords": [
    "inversify",
    "typescript",
    "typescript inject",
    "typescript di",
    "react di",
    "vue di",
    "vue inject",
    "react inject",
    "litelement",
    "polymer",
    "support ES Module"
  ]
}
