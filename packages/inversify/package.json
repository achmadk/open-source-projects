{
  "version": "1.0.4",
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.umd.cjs",
  "typings": "./dist/index.d.ts",
  "homepage": "https://github.com/achmadk/open-source-projects/tree/main/packages/inversify#readme",
  "sideEffects": false,
  "exports": {
    ".": {
      "node": "./dist/index.js",
      "import": "./dist/index.js",
      "require": "./dist/index.umd.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": ["dist"],
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "start": "vite",
    "build": "vite build",
    "test": "vitest run --coverage",
    "lint": "biome check --organize-imports-enabled=false --no-errors-on-unmatched --skip-errors --write --unsafe",
    "prepublishOnly": "pnpm run build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "dependencies": {
    "@scarf/scarf": "1.3.0"
  },
  "peerDependencies": {
    "@abraham/reflection": "0.12.0"
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "@achmadk/inversify",
  "author": "Achmad Kurnianto",
  "module": "./dist/index.js",
  "size-limit": [
    {
      "path": "./dist/index.js",
      "limit": "800 B"
    },
    {
      "path": "./dist/index.umd.cjs",
      "limit": "10 kB"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/achmadk/open-source-projects.git",
    "directory": "packages/inversify"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "ioc",
    "di",
    "javascript",
    "typescript",
    "node",
    "dependency injection",
    "dependency inversion",
    "inversion of control container"
  ]
}
