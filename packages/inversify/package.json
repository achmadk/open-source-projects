{
	"version": "2.0.2",
	"license": "MIT",
	"type": "module",
	"main": "dist/index.js",
	"typings": "dist/index.d.ts",
	"sideEffects": false,
	"exports": {
		".": {
			"node": "./dist/inversify-esm.esm.js",
			"import": "./dist/inversify-esm.esm.js",
			"require": "./dist/index.js",
			"types": "./dist/index.d.ts"
		}
	},
	"files": ["dist"],
	"engines": {
		"node": ">=14"
	},
	"scripts": {
		"start": "dts watch",
		"build": "dts build",
		"pretest": "npm run lint",
		"test": "dts test",
		"lint": "dts lint --fix",
		"prepare": "dts build",
		"prepublishOnly": "dts build",
		"size": "size-limit",
		"analyze": "size-limit --why"
	},
	"peerDependencies": {
		"@abraham/reflection": "0.10.0"
	},
	"husky": {
		"hooks": {
			"pre-commit": "dts lint --fix"
		}
	},
	"prettier": {
		"printWidth": 80,
		"semi": true,
		"singleQuote": true,
		"trailingComma": "es5"
	},
	"name": "@achmadk/inversify",
	"author": "Achmad Kurnianto",
	"module": "dist/inversify-esm.esm.js",
	"size-limit": [
		{
			"path": "dist/inversify-esm.cjs.production.min.js",
			"limit": "14 KB"
		},
		{
			"path": "dist/inversify-esm.esm.js",
			"limit": "70 B"
		}
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/achmadk/inversify-esm.git"
	},
	"devDependencies": {
		"@abraham/reflection": "0.12.0",
		"@size-limit/preset-small-lib": "^11.1.4",
		"@size-limit/webpack": "11.1.4",
		"@size-limit/webpack-why": "11.1.4",
		"@skypack/package-check": "0.2.2",
		"@types/jest": "29.5.12",
		"@typescript-eslint/eslint-plugin": "7.15.0",
		"@typescript-eslint/parser": "7.15.0",
		"babel-eslint": "10.1.0",
		"dts-cli": "2.0.5",
		"eslint": "8.23.1",
		"eslint-config-prettier": "9.1.0",
		"eslint-config-react-app": "7.0.1",
		"eslint-plugin-flowtype": "8.0.3",
		"eslint-plugin-import": "2.29.1",
		"eslint-plugin-jsx-a11y": "6.9.0",
		"eslint-plugin-prettier": "5.1.3",
		"eslint-plugin-react": "7.34.3",
		"eslint-plugin-react-hooks": "4.6.2",
		"husky": "^9.0.11",
		"jest": "29.7.0",
		"jest-watch-typeahead": "2.2.2",
		"prettier": "3.3.2",
		"size-limit": "^11.1.4",
		"ts-jest": "29.1.5",
		"tslib": "^2.6.3",
		"typescript": "^4.9.5"
	},
	"keywords": [
		"ioc",
		"di",
		"javascript",
		"typescript",
		"node",
		"dependency injection",
		"dependency inversion",
		"inversion of control container"
	]
}
