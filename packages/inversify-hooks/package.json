{
  "version": "1.0.0",
  "license": "MIT",
  "main": "dist/index.umd.cjs",
  "type": "module",
  "typings": "dist/index.d.ts",
  "types": "dist/index.d.ts",
  "files": ["dist"],
  "sideEffects": false,
  "exports": {
    ".": {
      "node": "./dist/index.umd.cjs",
      "import": "./dist/index.js",
      "require": "./dist/index.umd.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "start": "dts watch",
    "build": "vite build",
    "test": "vitest run",
    "lint": "biome check --organize-imports-enabled=false --no-errors-on-unmatched --skip-errors --write --unsafe",
    "prepublishOnly": "pnpm run build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "dependencies": {
    "@scarf/scarf": "1.3.0"
  },
  "peerDependencies": {
    "@abraham/reflection": "0.12.0",
    "@achmadk/inversify": "workspace:../inversify",
    "@achmadk/inversify-props": "workspace:../inversify-props",
    "react": "17.x || 18.x"
  },
  "name": "@achmadk/inversify-hooks",
  "author": "Achmad Kurnianto",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/achmadk/open-source-projects.git",
    "directory": "packages/inversify-hooks"
  },
  "publishConfig": {
    "access": "public"
  },
  "module": "dist/index.js",
  "size-limit": [
    {
      "path": "dist/index.js",
      "limit": "55 B"
    },
    {
      "path": "dist/index.umd.cjs",
      "limit": "460 B"
    }
  ],
  "devDependencies": {
    "@abraham/reflection": "0.12.0",
    "@achmadk/inversify": "workspace:../inversify",
    "@achmadk/inversify-props": "workspace:../inversify-props",
    "@size-limit/preset-small-lib": "^11.1.4",
    "@size-limit/webpack": "11.1.4",
    "@size-limit/webpack-why": "11.1.4",
    "@skypack/package-check": "0.2.2",
    "@testing-library/jest-dom": "^6.4.6",
    "@testing-library/react": "^16.0.0",
    "@types/react": "18.3.3",
    "dts-cli": "2.0.5",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "size-limit": "^11.1.4"
  },
  "keywords": [
    "inversify",
    "typescript",
    "typescript inject",
    "typescript di",
    "react di",
    "react inject",
    "react hooks",
    "es module support"
  ]
}
